{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rch-databricks"
		},
		"rch_adls_gen2_csv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'rch_adls_gen2_csv'"
		},
		"rch_adls_gen2_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rchadlsgen2.dfs.core.windows.net"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/rch_adls_gen2_csv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "RCH ADLS Gen2 csv",
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('rch_adls_gen2_csv_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('rch_adls_gen2_csv_accountKey')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}